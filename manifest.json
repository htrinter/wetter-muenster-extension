{
  "name": "Wetter Münster",
  "version": "1.2.2",
  "manifest_version": 2,
  "description": "Aktuelle Wetterdaten des Instituts für Landschaftsökologie der Universität Münster.",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.uni-muenster.de/Klima/wetter/wetter.php",
    "storage",
    "alarms"
  ],  
  "background": {
    "scripts": ["jquery.min.js", "eventpage.js"],
    "persistent": false
  }
}